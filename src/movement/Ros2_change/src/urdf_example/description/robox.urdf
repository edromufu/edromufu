<?xml version="1.0"?>

<robot name="aurea">

  <material name="black">
    <color rgba="0 0 0 1"/>
  </material>
  <material name="white">
    <color rgba="1 1 1 1"/>
  </material>
  <material name="red">
    <color rgba="1 0 0 0.2"/>
  </material>
  <material name="blue">
    <color rgba="0 0 1 0.2"/>
  </material>
  <material name="green">
    <color rgba="0 1 0 0.2"/>
  </material>

  <link name="base_link">
    <visual>
      <geometry>
        <sphere radius="0.015"/>
      </geometry>
      <material name="white"/>
    </visual>
  </link>

  <link name="x_axis">
    <visual>
      <geometry>
				<cylinder radius="0.015" length="5"/>
			</geometry>
      <origin rpy="0 1.57075 0" xyz="2.5 0 0"/>
      <material name="red"/>
    </visual>
  </link>

	<joint name="base_to_x_axis" type="fixed">
    <parent link="base_link"/>
    <child link="x_axis"/>
    <origin xyz="0 0 0"/>
  </joint>

  <link name="y_axis">
    <visual>
      <geometry>
				<cylinder radius="0.015" length="5"/>
			</geometry>
      <origin rpy="1.57075 0 0" xyz="0 2.5 0"/>
      <material name="green"/>
    </visual>
  </link>

	<joint name="base_to_y_axis" type="fixed">
    <parent link="base_link"/>
    <child link="y_axis"/>
    <origin xyz="0 0 0"/>
  </joint>

  <link name="z_axis">
    <visual>
      <geometry>
				<cylinder radius="0.015" length="5"/>
			</geometry>
      <origin xyz="0 0 2.5"/>
      <material name="blue"/>
    </visual>
  </link>

  <joint name="base_to_z_axis" type="fixed">
    <parent link="base_link"/>
    <child link="z_axis"/>
    <origin xyz="0 0 0"/>
  </joint>

  <link name="COM_height">
  </link>

  <joint name="COM_height_slider" type="prismatic">
    <axis xyz="0 0 1"/>
    <limit effort="1000.0" lower="-1" upper="1" velocity="10"/>
    <parent link="base_link"/>
    <child link="COM_height"/>
    <origin xyz="0 0 0.4"/>
  </joint>

  <link name="COM_pitch">
  </link>
  <joint name="COM_pitch_joint" type="revolute">
    <axis xyz="0 1 0"/>
    <limit effort="1000.0" lower="-3.14" upper="3.14" velocity="10"/>
    <parent link="COM_height"/>
    <child link="COM_pitch"/>
    <origin xyz="0 0 0"/>
  </joint>

  <link name="COM">
    <visual>
      <geometry>
				<cylinder radius="0.01" length="0.03"/>
			</geometry>
      <origin xyz="0 0 0"/>
      <material name="white"/>
    </visual>
  </link>

  <joint name="base_to_COM" type="fixed">
    <parent link="COM_pitch"/>
    <child link="COM"/>
    <origin xyz="0 0 0"/>
  </joint>

